package com.alten.shop.model.dto;

public class ProductDto {
	private Long id;
	private String name;
	private String description;
	private String image;
	private Double price;
	private String category;
	private String status;

	public ProductDto() {
	}

	public ProductDto(Long id, String name, String description, String image, Double price, String category,
			String status) {
		this.id = id;
		this.name = name;
		this.description = description;
		this.image = image;
		this.price = price;
		this.category = category;
		this.status = status;
	}
	
	
}
